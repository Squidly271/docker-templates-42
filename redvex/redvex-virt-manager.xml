<?xml version="1.0"?>
<Container version="2">
  <Name>redvex-virt-manager</Name>
  <Repository>mber5/virt-manager:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8888]/</WebUI>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1625131297</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/dev/kvm</HostDir>
      <ContainerDir>/dev/kvm</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/vman/root</HostDir>
      <ContainerDir>/root</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/var/lib/libvirt/images</HostDir>
      <ContainerDir>/var/lib/libvirt/images</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/var/run/libvirt/libvirt-sock</HostDir>
      <ContainerDir>/var/run/libvirt/libvirt-sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>['qemu:///system']</Value>
      <Name>HOSTS</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="/dev/kvm" Target="/dev/kvm" Default="" Mode="rw" Description="Container Device: " Type="Path" Display="always-hide" Required="false" Mask="false">/dev/kvm</Config>
  <Config Name="RootFolder" Target="/root" Default="" Mode="rw" Description="Container Path: /root" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/vman/root</Config>
  <Config Name="images" Target="/var/lib/libvirt/images" Default="" Mode="rw" Description="Container Path: /var/lib/libvirt/images" Type="Path" Display="always-hide" Required="false" Mask="false">/var/lib/libvirt/images</Config>
  <Config Name="libvirt-sock" Target="/var/run/libvirt/libvirt-sock" Default="" Mode="rw" Description="Container Path: /var/run/libvirt/libvirt-sock" Type="Path" Display="always-hide" Required="false" Mask="false">/var/run/libvirt/libvirt-sock</Config>
  <Config Name="Web-GUI" Target="80" Default="8888" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="false" Mask="false">8888</Config>
  <Config Name="HOSTS" Target="HOSTS" Default="" Mode="" Description="Container Variable: HOSTS" Type="Variable" Display="advanced" Required="false" Mask="false">['qemu:///system']</Config>
</Container>
